<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 100 100">
  <!-- 深蓝色背景 -->
  <rect width="100" height="100" fill="#1a1a5e" rx="8"/>
  
  <!-- 金色边框 -->
  <rect x="5" y="5" width="90" height="90" fill="none" stroke="#FFD700" stroke-width="3" rx="6"/>
  
  <!-- 中心金色星星 -->
  <g transform="translate(50, 50)">
    <!-- 大星星 -->
    <path d="M 0 -25 L 7 -7 L 25 -7 L 10 3 L 15 20 L 0 8 L -15 20 L -10 3 L -25 -7 L -7 -7 Z" 
          fill="#FFD700" stroke="#FFA500" stroke-width="1"/>
    <!-- 小星星装饰 -->
    <circle cx="-20" cy="-20" r="3" fill="#FFD700"/>
    <circle cx="20" cy="-20" r="3" fill="#FFD700"/>
    <circle cx="-20" cy="20" r="3" fill="#FFD700"/>
    <circle cx="20" cy="20" r="3" fill="#FFD700"/>
  </g>
  
  <!-- 金色装饰线条 -->
  <path d="M 20 20 L 25 20 L 22.5 25 Z" fill="#FFD700"/>
  <path d="M 80 20 L 75 20 L 77.5 25 Z" fill="#FFD700"/>
  <path d="M 20 80 L 25 80 L 22.5 75 Z" fill="#FFD700"/>
  <path d="M 80 80 L 75 80 L 77.5 75 Z" fill="#FFD700"/>
</svg>
