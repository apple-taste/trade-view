{
  "repo_url": "https://github.com/apple-taste/trade-view",
  "service_name": "trade-view",
  "branch": "main",
  "port": 8000,
  "streaming_log_timeout_seconds": 120,
  "env_vars": {
    "NODE_ENV": "production",
    "LOG_LEVEL": "info",
